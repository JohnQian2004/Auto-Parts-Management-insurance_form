  background-clip: text;
  -webkit-background-clip: text;
  background-size: 500%;
  animation: color 10s linear infinite, pulse 3s infinite;
}

@keyframes pulse {
  0% {
    -webkit-transform: scale(1.1);
    transform: scale(1.1);
  }

  50% {
    -webkit-transform: scale(0.9);
    transform: scale(0.9);
  }

  100% {
    -webkit-transform: scale(1);
    transform: scale(1);
  }
}

